<xsd:schema xmlns:hdf5="http://hdfgroup.org/HDF5/XML/schema/HDF5-File"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
    attributeFormDefault="unqualified">
    <xsd:element name="PolymerNanocomposite" type="Root">
        <xsd:annotation>
            <xsd:documentation>main polymer nanocomposite (PNC) sample entry </xsd:documentation>
        </xsd:annotation>
    </xsd:element>

    <xsd:complexType name="Root">
        <xsd:sequence>
            <xsd:element name="MATERIALS" type="MaterialsType">
                <xsd:annotation>
                    <xsd:documentation>Material constituent charateristics </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="SIMULATIONS" type="SimulationsType">
                <xsd:annotation>
                    <xsd:documentation>Simulation configurations and simulation-specific outputs</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="MaterialsType">
        <xsd:sequence>
<!--             <xsd:element minOccurs="0" name="Matrix" type="MatrixType" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Polymer matrix </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Filler" type="FillerType" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Nanoparticle filler</xsd:documentation>
                </xsd:annotation>
            </xsd:element> -->
            <xsd:element minOccurs="0" name="ModelSpecificTerms" type="ModelSpecificTermsType">
                <xsd:annotation>
                    <xsd:documentation>Materials system information that is specific to simulation models</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ComputationalStructureInputFile" type="InputFileType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ModelSpecificTermsType">
        <xsd:choice>
            <xsd:element name="MDAAMaterials" type="MDAAMaterialsType">
                <xsd:annotation>
                    <xsd:documentation>Materials system information that is specific to all-atomistic MD simulation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MDCGMaterials" type="MDCGMaterialsType">
                <xsd:annotation>
                    <xsd:documentation>Materials system information that is specific to coarse-grained MD simulation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <!-- FEA/DFT extra terms go here -->
        </xsd:choice>
    </xsd:complexType>

    <xsd:complexType name="MDAAMaterialsType">
        <xsd:sequence>
            <xsd:element name="NumberOfAtoms" type="PositiveValueUnitType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="StateAtRoomTemp" type="StateAtRoomTemperatureType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PolymerArchitectures" type="PolymerArchitecturesType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="DegreeOfCrystallization" type="DegreeOfCrystallizationQualitativeType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MonomerMass" type="PositiveValueUnitType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="DegreeOfPolymerization" type="PositiveValueUnitType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="NumberOfChains" type="PositiveValueUnitType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="BulkDensity" type="PositiveValueUnitType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="StateAtRoomTemperatureType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="gas"/>
            <xsd:enumeration value="low-viscosity liquid"/>
            <xsd:enumeration value="high-viscosity liquid"/>
            <xsd:enumeration value="simple soft solid"/>
            <xsd:enumeration value="tough plastic solid"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="PolymerArchitecturesType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="linear"/>
            <xsd:enumeration value="ring"/>
            <xsd:enumeration value="star-branched"/>
            <xsd:enumeration value="H-branched"/>
            <xsd:enumeration value="comb"/>
            <xsd:enumeration value="ladder"/>
            <xsd:enumeration value="dendrimer"/>
            <xsd:enumeration value="randomly-branched"/>
            <xsd:enumeration value="polymer-network"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="DegreeOfCrystallizationQualitativeType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="crystalline"/>
            <xsd:enumeration value="semi-crystalline"/>
            <xsd:enumeration value="amorphous"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="MDCGMaterialsType">
        <xsd:sequence>
            <xsd:element name="NumberOfCGBeadsForEachGraftChain" type="PositiveValueUnitType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="NumberOfCGBeadsForEachMatrixChain" type="PositiveValueUnitType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="NumberOfMatrixChains" type="PositiveValueUnitType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="NumberDensity" type="PositiveValueUnitType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="InputFileType">
        <xsd:sequence>
            <xsd:element name="description" type="xsd:string" minOccurs="0"/>
            <xsd:element name="File" minOccurs="0" xmlns:ns0="http://mdcs.ns" ns0:_mod_mdcs_="/curator/blob-hoster"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="SimulationsType">
        <xsd:sequence>
            <xsd:element name="MolecularDynamics" type="MolecularDynamicsType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Molecular dynamics simulation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <!-- FEA, DFT to be added here -->
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="MolecularDynamicsType">
        <xsd:sequence>
            <xsd:element name="MDSimulationModel" type="MDSimulationModelType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>MD simulation model configurations</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MDSimulationMethods" type="MDSimulationMethodsType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Methods used in the MD simulation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MDSimulationSystem" type="MDSimulationSystemType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>System defined for the MD simulation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MDMicrostructureOutput" type="MDMicrostructureOutputType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Microstructure output by the MD simulation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="MDSimulationModelType">
        <xsd:sequence>
            <xsd:element name="MDSoftware" type="MDSoftwareType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Software used for MD simulation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Dimension" type="DimensionType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Dimension for MD simulation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MDModelInputFile" type="InputFileType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Input files for MD simulation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ParticleTypeSpecifications" type="ParticleTypeSpecificationsType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AAModelConfiguration" type="AAModelConfigurationType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Configurations for all-atomistic MD simulation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="CGModelConfiguration" type="CGModelConfigurationType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Configurations for coarse-grained MD simulation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="MDSoftwareType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="NAMD"/>
            <xsd:enumeration value="LAMMPS"/>
            <xsd:enumeration value="Material Studio"/>
            <xsd:enumeration value="HOOMD-Blue"/>
            <xsd:enumeration value="GROMACS"/>
            <xsd:enumeration value="others"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="DimensionType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="2D"/>
            <xsd:enumeration value="3D"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="ParticleTypeSpecificationsType">
        <xsd:sequence>
            <xsd:element name="Particle" type="ParticleType" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ParticleType">
        <xsd:sequence>
            <xsd:element name="description" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Description of the particle</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ParticleType" type="ParticleTypePool">
                <xsd:annotation>
                    <xsd:documentation>Particle type of the particle</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="ParticleTypePool">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="type 1"/>
            <xsd:enumeration value="type 2"/>
            <xsd:enumeration value="type 3"/>
            <xsd:enumeration value="type 4"/>
            <xsd:enumeration value="type 5"/>
            <xsd:enumeration value="type 6"/>
            <xsd:enumeration value="type 7"/>
            <xsd:enumeration value="type 8"/>
            <xsd:enumeration value="type 9"/>
            <xsd:enumeration value="type 10"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="AAModelConfigurationType">
        <xsd:sequence>
            <xsd:element name="AAForcefieldName" type="AAForcefieldType">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="AAForcefieldType">
        <xsd:sequence>
            <xsd:element name="ForcefieldName" type="ForcefieldNamePool">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="value" type="xsd:double" minOccurs="0"/>
            <xsd:element name="unit" type="xsd:string" minOccurs="0"/>
            <xsd:element minOccurs="0" name="uncertainty" type="Uncertainty"/>
            <xsd:element minOccurs="0" name="datafile" type="InputFileType"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="ForcefieldNamePool">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="CHARMM"/>
            <xsd:enumeration value="AMBER"/>
            <xsd:enumeration value="ECEPP"/>
            <xsd:enumeration value="GROMOS"/>
            <xsd:enumeration value="CFF95"/>
            <xsd:enumeration value="MM3"/>
            <xsd:enumeration value="MMFF94"/>
            <xsd:enumeration value="DREIDING"/>
            <xsd:enumeration value="UFF"/>
            <xsd:enumeration value="others"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="CGModelConfigurationType">
        <xsd:sequence>
            <xsd:element name="CGPotentialEnergyUnitType" type="CGPotentialEnergyUnitType">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="BondPotentialEnergy" type="BondPotentialEnergyType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="NonBondedPotentialEnergy" type="NonBondedPotentialEnergyType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AnglePotentialEnergy" type="AnglePotentialEnergyType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="OtherCGPotentialEnergy" type="OtherCGPotentialEnergyType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="CGPotentialEnergyUnitType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="LJ"/>
            <xsd:enumeration value="real"/>
            <xsd:enumeration value="SI"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="BondPotentialEnergyType">
        <xsd:sequence>
            <xsd:element name="BondStyle" type="BondStylePool">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="BondType" type="ParticleTypePool" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="BondCoefficient" type="PositiveValueType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="EquilibriumBondLength" type="PositiveValueType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PotentialWellDepth" type="PositiveValueType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PotentialEnergyAtDistanceZero" type="PositiveValueType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ShiftParameter" type="ScalarUncertainty" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="BondStylePool">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="fene"/>
            <xsd:enumeration value="fene/expand"/>
            <xsd:enumeration value="harmonic"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="NonBondedPotentialEnergyType">
        <xsd:sequence>
            <xsd:element name="PairStyle" type="PairStylePool">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ParticleType1" type="ParticleTypePool" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ParticleType2" type="ParticleTypePool" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PotentialWellDepth" type="PositiveValueType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PotentialEnergyAtDistanceZero" type="PositiveValueType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ShiftParameter" type="ScalarUncertainty" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="LJCutoffDistance" type="PositiveValueType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="PairStylePool">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="lj/cut"/>
            <xsd:enumeration value="lj/expand"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="AnglePotentialEnergyType">
        <xsd:sequence>
            <xsd:element name="AngleStyle" type="AngleStylePool">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AngleType" type="ParticleTypePool" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AngleCoefficient" type="PositiveValueType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="EquilibriumAngle" type="PositiveValueType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="AngleStylePool">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="harmonic"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="OtherCGPotentialEnergyType">
        <xsd:sequence>
            <xsd:element name="OtherCGPotentialEnergyStyle" type="InputFileType">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ParticleType1" type="ParticleTypePool" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ParticleType2" type="ParticleTypePool" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>



    <xsd:complexType name="MDSimulationMethodsType">
        <xsd:sequence>
            <xsd:element name="MDMechanicalMethod" type="MDMechanicalMethodType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MDTransportMethod" type="MDTransportMethodType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MDThermodynamicMethod" type="MDThermodynamicMethodType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MDStructralMethod" type="MDStructralMethodType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MDThermalMethod" type="MDThermalMethodType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MDDynamicApproachRelaxationTime" type="TimeType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MDEquilibriumMethod" type="MDEquilibriumMethodType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="MDMechanicalMethodType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="non-equilibrium molecular dynamics"/>
            <xsd:enumeration value="stress fluctuation"/>
            <xsd:enumeration value="strain fluctuation"/>
            <xsd:enumeration value="stress-strain fluctuation"/>
            <xsd:enumeration value="steered molecular dynamics"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="MDTransportMethodType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="non-equilibrium molecular dynamics"/>
            <xsd:enumeration value="equilibrium Green-Kubo method"/>
            <xsd:enumeration value="Einstein form"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="MDThermodynamicMethodType">
        <xsd:restriction base="xsd:string"/>
    </xsd:simpleType>

    <xsd:simpleType name="MDStructralMethodType">
        <xsd:restriction base="xsd:string"/>
    </xsd:simpleType>

    <xsd:simpleType name="MDThermalMethodType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="thermal dynamic approach"/>
            <xsd:enumeration value="dynamic approach"/>
            <xsd:enumeration value="DMA"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="MDEquilibriumMethodType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="energy minimization"/>
            <xsd:enumeration value="Monte Carlo method"/>
            <xsd:enumeration value="molecular dynamics method"/>
            <xsd:enumeration value="others"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="MDSimulationSystemType">
        <xsd:sequence>
            <xsd:element name="MDSimulationBoxSize" type="MDSimulationBoxSizeType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MDSimulationBoundaryConditions" type="MDSimulationBoundaryConditionsGeneralType" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MDEnsemblesForEquilibriumProcess" type="MDEnsemblesGeneralType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MDEnsemblesForProduction" type="MDEnsemblesGeneralType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="MDSimulationBoxSizeType">
        <xsd:sequence>
            <xsd:element name="MDSimulationBoxSizeX" type="DistanceType">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MDSimulationBoxSizeY" type="DistanceType">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MDSimulationBoxSizeZ" type="DistanceType">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="MDSimulationBoundaryConditionsGeneralType">
        <xsd:sequence>
            <xsd:element name="MDBoundaryConditionType" type="MDBoundaryConditionType">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MDBoundaryConditionDirection" type="MDBoundaryConditionDirectionType">
                <xsd:annotation>
                    <xsd:documentation></xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="MDBoundaryConditionType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="p (periodic)"/>
            <xsd:enumeration value="f (non-periodic and fixed)"/>
            <xsd:enumeration value="s (non-periodic and shrink-wrapped)"/>
            <xsd:enumeration value="m (non-periodic and shrink-wrapped with a minimum value)"/>
        </xsd:restriction>
